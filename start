#!/bin/bash

# Color code
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m' 
BLUE='\033[0;34m'
PURPLE='\033[0;35m'
CYAN='\033[0;36m'
PINK='\033[1;35m' 
NC='\033[0m' 

echo -e "
${RED} _    _ _     _____   _____ _____  ______  ___ ______  ___  ____   _ _____ _____ _____  ${NC}
${GREEN}| |  | | |   /  __ \ |_   _|  _  | | ___ \/ _ \|  _  \ |  \/  | | | /  ___|_   _/  __ \ ${NC}
${BLUE}| |  | | |   | /  \/   | | | | | | | |_/ / /_\ \ | | | | .  . | | | \ \`--.  | | | /  \/ ${NC}
${PURPLE}| |/\| | |   | |       | | | | | | | ___ \  _  | | | | | |\/| | | | |\`--. \ | | | |     ${NC}
${PINK}\\  /\\  / |___| \\__/\   | | \\ \\_/ / | |_/ / | | | |/ /  | |  | | |_| /\\__/ /_| |_| \\__/\\ ${NC}
${CYAN} \\/  \\/\\_____/\\____/   \\_/  \\___/  \\____/\\_| |_/___/   \\_|  |_/\\___/\\____/ \\___/ \\____/ ${NC}
"

python3 -m Music
